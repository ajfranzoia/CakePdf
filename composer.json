	"homepage": "http://github.com/ceeram/CakePdf",
	"license": "MIT",
	"require": {
		"php": ">=5.3.0",
		"composer/installers": "*"
	},
        "suggested": {
            "dompdf/dompdf": "dev-master",
            "tecnick.com/tcpdf": "6.0.031",
            "mpdf/mpdf": "v5.7.1"
        },
	"extra": {
		"installer-name": "CakePdf"
	}
}
